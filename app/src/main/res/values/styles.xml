<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomEdittextStyle">
        <item name="android:textSize">@dimen/edittextsize</item>
        <item name="android:textColor">#808080</item>
        <item name="android:textColorHint">#808080</item>
        <item name="android:drawablePadding">10dp</item>
    </style>
    <style name="CustomTextView">
        <item name="android:textSize">@dimen/textviewtextsize</item>
        <item name="android:textColor">#808080</item>
        <item name="bottomSheetStyle">@style/Custom_Bottom_Sheet_Style</item>
    </style>
    <style name="Custom_Bottom_Sheet_Style">
        <item name="android:background">@drawable/bottom_sheet_bg</item>
    </style>
    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>